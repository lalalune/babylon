{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "../src/a2a",
    "../src/agents",
    "../src/lib",
    "../src/engine",
    "../plugin-babylon/src"
  ],
  "entryPointStrategy": "expand",
  "out": "app/reference/_generated/tsdoc",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/dist/**",
    "**/build/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "A2A Protocol",
    "Agents",
    "Game Engine",
    "API",
    "Utilities",
    "*"
  ],
  "groupOrder": [
    "Classes",
    "Functions",
    "Interfaces",
    "Type Aliases",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Class",
    "Interface",
    "TypeAlias",
    "Function",
    "Variable"
  ]
}

