# Test Environment Variables Template
# Copy this file to .env.test and fill in your test values
# NEVER commit .env.test to git!

# Database (use separate test database)
DATABASE_URL=postgresql://user:password@localhost:5432/babylon_test
POSTGRES_PRISMA_URL=postgresql://user:password@localhost:5432/babylon_test
POSTGRES_URL_NON_POOLING=postgresql://user:password@localhost:5432/babylon_test

# NextAuth
NEXTAUTH_SECRET=your-test-secret-here-minimum-32-characters

# Privy (use test app or separate test credentials)
PRIVY_APP_ID=your-privy-test-app-id
PRIVY_APP_SECRET=your-privy-test-app-secret
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-test-app-id

# Privy Test User (create a dedicated test user)
PRIVY_TEST_EMAIL=test@example.com
PRIVY_TEST_PASSWORD=TestPassword123!

# AI Services (use separate test API keys with rate limits)
ANTHROPIC_API_KEY=your-anthropic-test-key
OPENAI_API_KEY=your-openai-test-key
OPENPIPE_API_KEY=your-openpipe-test-key

# RL Training Configuration
BASE_MODEL=OpenPipe/Qwen3-14B-Instruct
WANDB_API_KEY=your-wandb-api-key
WANDB_PROJECT=babylon-rl-training
WANDB_ENTITY=your-wandb-entity
# Minimum new trajectories required before cron triggers training
TRAINING_MIN_TRAJECTORIES=1000
# Automatically create embedded wallets for test agents (requires Privy env vars)
AUTO_CREATE_AGENT_WALLETS=true

# Test Wallet (NEVER use real funds)
WALLET_SEED_PHRASE=test test test test test test test test test test test junk
WALLET_PASSWORD=TestPassword123!

# Optional: Vercel KV for caching (can use test instance)
KV_URL=
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Optional: Other services (use test instances)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

