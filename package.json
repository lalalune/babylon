{
  "name": "babylon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "bun run scripts/pre-dev/pre-dev-local.ts && concurrently -n \"next,cron\" -c \"cyan,magenta\" \"next dev\" \"bun run scripts/local-cron-simulator.ts\"",
    "dev:testnet": "DEPLOYMENT_ENV=testnet bun run scripts/pre-dev/pre-dev-testnet.ts && concurrently -n \"next,cron\" -c \"cyan,magenta\" \"next dev\" \"bun run scripts/local-cron-simulator.ts\"",
    "build": "prisma generate && NODE_OPTIONS='--max-old-space-size=8192' next build --webpack",
    "start": "bun run scripts/validation/check-environment.ts && next start",
    "test": "bun test tests/unit/ tests/integration/ tests/deployment/ tests/markets-pnl-sharing.test.ts src/a2a/tests/",
    "test:testnet": "bun test tests/deployment/testnet.test.ts tests/integration/agent0-testnet.test.ts",
    "test:mainnet": "bun test tests/deployment/mainnet.test.ts",
    "test:e2e": "playwright test tests/e2e && playwright test --config=synpress.config.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "bun run prisma/seed.ts",
    "deploy:local": "bun run scripts/deployment/deploy-localnet.ts",
    "deploy:testnet": "bun run scripts/deployment/deploy-testnet.ts",
    "deploy:mainnet": "bun run scripts/deployment/deploy-mainnet.ts",
    "contracts:test": "forge test",
    "agent0:setup": "bun run scripts/agent0/configure-agent0.ts && bun run scripts/register-babylon.ts",
    "testnet:setup": "bun run scripts/post-deploy/testnet-setup.ts"
  },
