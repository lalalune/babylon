{
  "name": "babylon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "bun run scripts/pre-dev/pre-dev-local.ts && concurrently -n \"next,cron\" -c \"cyan,magenta\" \"bunx next dev\" \"bun run scripts/local-cron-simulator.ts\"",
    "dev:next-only": "SKIP_ENV_VALIDATION=1 bunx next dev",
    "dev:testnet": "DEPLOYMENT_ENV=testnet bun run scripts/pre-dev/pre-dev-testnet.ts && concurrently -n \"next,cron\" -c \"cyan,magenta\" \"bunx next dev\" \"bun run scripts/local-cron-simulator.ts\"",
    "build": "prisma generate && NODE_OPTIONS='--max-old-space-size=8192' bunx next build --webpack",
    "start": "bun run scripts/validation/check-environment.ts && bunx next start",
    "test": "bun test tests/unit/ tests/integration/ tests/deployment/ tests/markets-pnl-sharing.test.ts src/lib/__tests__/",
    "test:retry": "bun test src/lib/__tests__/prisma-retry.integration.test.ts",
    "test:a2a": "bun test src/a2a/tests/",
    "test:a2a:unit": "bun test ./src/a2a/tests/integration/points-service-integration.test.ts",
    "test:a2a:live": "bun run src/a2a/tests/live-server.test.ts",
    "test:plugin": "bun test tests/integration/babylon-plugin-complete.test.ts",
    "test:testnet": "bun test tests/deployment/testnet.test.ts tests/integration/agent0-testnet.test.ts",
    "test:mainnet": "bun test tests/deployment/mainnet.test.ts",
    "test:e2e": "playwright test tests/e2e && playwright test --config=synpress.config.ts",
    "test:ci": "bash scripts/ci/run-all-tests.sh",
    "test:playwright": "playwright test tests/e2e",
    "test:synpress": "playwright test --config=synpress.config.ts",
    "test:logout": "playwright test --config=synpress.config.ts tests/synpress/08-logout-verification.spec.ts",
    "test:logout:headed": "playwright test --config=synpress.config.ts tests/synpress/08-logout-verification.spec.ts --headed",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "bun run prisma/seed.ts",
    "deploy:local": "bun run scripts/deployment/deploy-localnet.ts",
    "deploy:testnet": "bun run scripts/deployment/deploy-testnet.ts",
    "deploy:mainnet": "bun run scripts/deployment/deploy-mainnet.ts",
    "contracts:test": "forge test",
    "contracts:test:oracle": "forge test --match-contract BabylonGameOracleTest",
    "contracts:deploy:local": "bun run scripts/deployment/deploy-localnet.ts",
    "oracle:test:simple": "./scripts/test-oracle-simple.sh",
    "oracle:test:e2e": "bun test tests/integration/oracle-full-flow.test.ts",
    "oracle:test:betting": "bun test tests/integration/betting/ tests/e2e/betting-on-chain.test.ts",
    "oracle:test:integration": "bun test tests/integration/oracle/",
    "oracle:deploy-and-test": "./scripts/deploy-and-test-full.sh",
    "agent0:setup": "bun run scripts/agent0/configure-agent0.ts",
    "admin": "bun run scripts/admin.ts",
    "status": "bun run scripts/status.ts",
    "game:start": "bun run scripts/game-control-db.ts start",
    "game:pause": "bun run scripts/game-control-db.ts pause",
    "game:status": "bun run scripts/game-control-db.ts status",
    "db:start": "bun run scripts/db.ts start",
    "db:stop": "bun run scripts/db.ts stop",
    "db:status": "bun run scripts/db.ts status",
    "testnet:setup": "bun run scripts/post-deploy/testnet-setup.ts",
    "command:validate": "bun run src/cli/validate-actors.ts",
    "command:images": "bun run src/cli/generate-actor-images.ts",
    "command:simulate": "bun run src/cli/run-game.ts",
    "command:simulate:batch": "bun run src/cli/run-game.ts --count=100 --fast",
    "command:world": "bun run src/cli/generate-world.ts",
    "command:world:verbose": "bun run src/cli/generate-world.ts --verbose",
    "command:generate": "bun run src/cli/generate-game.ts",
    "command:generate:verbose": "bun run src/cli/generate-game.ts --verbose",
    "stress-test": "bun run scripts/run-load-test.ts",
    "stress-test:a2a": "bun run scripts/run-a2a-stress-test.ts",
    "stress-test:a2a:light": "bun run scripts/run-a2a-stress-test.ts light",
    "stress-test:a2a:normal": "bun run scripts/run-a2a-stress-test.ts normal",
    "stress-test:a2a:heavy": "bun run scripts/run-a2a-stress-test.ts heavy",
    "stress-test:a2a:rate-limit": "bun run scripts/run-a2a-stress-test.ts rate-limit",
    "stress-test:a2a:coalition": "bun run scripts/run-a2a-stress-test.ts coalition"
  },
